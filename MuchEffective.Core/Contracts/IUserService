using MuchEffective.Core.Entities;

namespace MuchEffective.Core.Contracts;

public interface IUserService {
    /// <summary>
    /// Интерфейс сервиса для работы с пользователеми
    /// </summary>
    /// <param name="userId"></param>
    /// <param name="userId1"></param>
    /// <returns></returns>
    Task<User> GetUser(string Guid, string Guid);
    Task InviteUserIntoProject(string Guid, string Guid, string Guid);
    Task ExcludeUserFromProject(string Guid, string Guid, string Guid);
}